<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="ShipIM" id="09-04-2024--2-create-image-filter-table">
        <createTable tableName="image_filter">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints
                        primaryKey="true"/>
            </column>
            <column name="status" type="VARCHAR">
                <constraints
                        nullable="false"/>
            </column>
            <column name="original_image_id" type="VARCHAR">
                <constraints
                        nullable="false"/>
            </column>
            <column name="modified_image_id" type="VARCHAR"/>
            <column name="request_id" type="VARCHAR">
                <constraints
                        unique="true"
                        nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>
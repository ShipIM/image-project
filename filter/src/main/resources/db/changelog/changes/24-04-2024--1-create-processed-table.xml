<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="ShipIM" id="24-04-2024--1-create-processed-table">
        <createTable tableName="processed">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints
                        primaryKey="true"/>
            </column>
            <column name="original_image_id" type="VARCHAR">
                <constraints
                        nullable="false"/>
            </column>
            <column name="request_id" type="VARCHAR">
                <constraints
                        nullable="false"/>
            </column>
        </createTable>
        <addUniqueConstraint
                columnNames="original_image_id, request_id"
                constraintName="unique_processed"
                tableName="processed"/>
    </changeSet>
</databaseChangeLog>